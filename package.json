    "electron-builder": "^22.14.13",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.7",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-storybook": "^0.6.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.0.2",
    "sass-resources-loader": "^2.2.5",
    "style-loader": "^3.3.1",
