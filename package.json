    "ahooks": "^3.7.1",
    "antd": "^4.23.4",
    "classnames": "^2.3.1",
    "electron-log": "^4.4.8",
    "electron-splashscreen": "^1.0.0",
    "electron-updater": "^4.6.5",
    "node-forge": "^1.3.1",
    "pinyin-match": "^1.2.2",
    "react-router-dom": "6",
    "react-spring": "^9.5.3",
    "sass": "^1.54.9",
    "tdesign-icons-react": "^0.1.6",
    "tdesign-react": "^0.40.6",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@storybook/addon-actions": "^6.5.10",
